<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sentInfoFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textSentNumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:textSize="24sp" />

        <TextView
            android:id="@+id/textKodSent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textSentMasa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textSentNazwaKlienta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:textSize="24sp" />

        <TextView
            android:id="@+id/textSentKierowca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:textSize="24sp" />

        <TextView
            android:id="@+id/textSentNrZlecMarketingowego"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="24sp"
             />

        <TextView
            android:id="@+id/textSentNrZlecSam"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="24sp"
             />

    </LinearLayout>

</FrameLayout>